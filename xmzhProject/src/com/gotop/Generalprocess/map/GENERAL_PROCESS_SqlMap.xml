  	 <select id="queryGeneralprocessList" resultMap="TGeneralProcess_HashMapResult" parameterClass="java.util.HashMap">
  		SELECT T11.*,v2.begin_empid,
               v2.begin_orgid,
               v2.cust_name,
               v2.currency,
               v2.apply_bal,
               v2.onecategory,
               v2.loancategory,
               v2.orgname as beginEmpname,
               v2.empname as beginOrgname FROM 
               (select *
  from (select T1.ID,
               T1.EXECUTION_ID,
               T1.TASK_ID,
               T1.PRE_TASK_ID,
               T1.PRE_TASK_ASSINGEE,
               T1.PRE_TASK_ORG,
               T1.PRE_TASK_TIME,
               T1.NEXT_TASK_ID,
               T1.BUSINESS_KEY,
               T3.EMPNAME AS preTaskAssingeeName,
               T10.NEXT_ORG_NAME AS preTaskOrgName,
               t6.process_name AS processName,
               t9.business_title,
               t6.business_type,
               t8.activity_name_ AS activityName,
               t1.NEXT_ACTIVITY_NAME as currentActivityName,
               to_char(T8.START_, 'yyyymmddhh24miss') AS startTime,
               to_char(T8.END_, 'yyyymmddhh24miss') AS endTime,
               t1.NEXT_ASSINGEE_NAME as currentAssingee,
               T8.END_,
               row_number() over(partition by t1.execution_id order by t1.id desc) rn
          from JBPM4_HIST_PROCINST     t5,
               T_PROCESS_CONFIG        t6,
               T_PROCESS_BUSINESS      t9,
               T_PROCESS_TASK_ASSIGNEE t1,
               JBPM4_HIST_TASK         t7,
               om_employee             t3,
               JBPM4_HIST_ACTINST      t8,
               T_APPROVE_OPNINION_GP t10
         where t5.procdefid_ = t6.definition_id
           and t9.execution_id = t5.id_
           and t1.execution_id = t9.execution_id
           and t7.execution_ = t1.execution_id
           and t3.empid = t1.pre_task_assingee
           AND T9.BUSINESS_TYPE=88
           and t8.htask_ = t1.pre_task_id
           and t10.node_id=t1.next_task_id
           )
 where rn = 1
) T11,V_APP_CONDITION V1,V_GENERALPROCESS_XD V2
 WHERE T11.EXECUTION_ID=V1.execution_id(+)
 AND T11.EXECUTION_ID=V2.flow_id(+)  order by END_ desc
  	</select> 
  	
  	<!-- 客户经理查询到自己发起的流程 -->
  	<select id="myStartGeneralProcessList" resultMap="TGeneralProcess_HashMapResult" parameterClass="java.util.HashMap">
  		select *
  from (select v.begin_empid,
               v.begin_orgid,
               v.cust_name,
               v.currency,
               v.apply_bal,
               v.onecategory,
               v.loancategory,
               v.orgname as beginEmpname,
               v.empname as beginOrgname,
               T1.ID,
               T1.EXECUTION_ID,
               T1.PRE_TASK_ID,
               T1.PRE_TASK_ASSINGEE,
               T1.PRE_TASK_ORG,
               T1.PRE_TASK_TIME,
               T1.NEXT_TASK_ID,
               T1.BUSINESS_KEY,
               T3.EMPNAME AS preTaskAssingeeName,
               T4.ORGNAME AS preTaskOrgName,
               t6.process_name AS processName,
               t9.business_title,
               t6.business_type,
               t8.activity_name_ AS activityName,
               t1.NEXT_ACTIVITY_NAME as currentActivityName,
               t1.NEXT_ASSINGEE_NAME as currentAssingee,
               T8.END_,
               row_number() over(partition by t1.execution_id order by t1.id desc) rn
          from JBPM4_HIST_PROCINST     t5,
               T_PROCESS_CONFIG        t6,
               T_PROCESS_BUSINESS      t9,
               T_PROCESS_TASK_ASSIGNEE t1,
               T_PROCESS_TASK_ASSIGNEE_PERSON t2,
               JBPM4_HIST_TASK         t7,
               om_employee             t3,
               OM_ORGANIZATION         T4,
               JBPM4_HIST_ACTINST      t8,
               om_emporg t10,
               v_generalprocess_xd     v
         where t5.procdefid_ = t6.definition_id
           and t9.execution_id = t5.id_
           and t1.execution_id = t9.execution_id
           and t7.execution_ = t1.execution_id
           and t3.empid = t1.pre_task_assingee
           and t2.task_id=t1.next_task_id
           and t8.htask_ = t1.pre_task_id
           and t9.execution_id = v.flow_id(+)
           and t6.business_type = 88
           and t10.orgid=t4.orgid
           and t10.empid=t2.task_assignee_id
           <isNotNull prepend="and" property="empId" >
              t9.submit_id= $empId$
      		</isNotNull>
           <isNotNull prepend="and" property="appTimeStrat" >
           	<![CDATA[
            	v.appTime>='$appTimeStrat$'
               ]]>
      		</isNotNull>
           <isNotNull prepend="and" property="appTimeEnd" >
           	<![CDATA[
            	v.appTime<='$appTimeEnd$'
               ]]>
      		</isNotNull>
           <isNotNull prepend="and" property="reporttimeStrat" >
           	 <![CDATA[
            	v.reporttimeStrat>='$reporttimeStrat$'
               ]]>
      		</isNotNull>
           <isNotNull prepend="and" property="reporttimeEnd" >
           	<![CDATA[
            	v.reporttimeStrat<='$reporttimeEnd$'
               ]]>
      		</isNotNull>
           <isNotNull prepend="and" property="orgCodeOne" >
           	v.orgCodeOne='$orgCodeOne$'
      		</isNotNull>
           <isNotNull prepend="and" property="orgCodeTwo" >
           	v.orgCodeTwo='$orgCodeTwo$'
      		</isNotNull>
           <isNotNull prepend="and" property="cust_Name" >
           	v.cust_Name like '%'||'$cust_Name$'||'%'
      		</isNotNull>
           <isNotNull prepend="and" property="isEnd" >
           	<isEqual property="isEnd" compareValue="0">
           	    t1.NEXT_ACTIVITY_NAME!='结束'
           	</isEqual>
           	<isEqual property="isEnd" compareValue="1">
           	    t1.NEXT_ACTIVITY_NAME='结束'
           	</isEqual>
      		</isNotNull>
           <isNotNull prepend="and" property="creator" >
           	v.creator='$creator$'
      		</isNotNull>
           <isNotNull prepend="and" property="fdxdy" >
           	v.fdxdy='$fdxdy$'
      		</isNotNull>
           
           ) t11,v_app_condition v2 
 where rn = 1
  and t11.execution_id=v2.execution_id(+)
  			<isNotNull prepend="and" property="dyApp" >
           	v2.sp1='$dyApp$'
      		</isNotNull>
           <isNotNull prepend="and" property="deApp" >
           	v2.sp2='$dyApp$'
      		</isNotNull>
           <isNotNull prepend="and" property="dyCheck" >
           v2.sc1='$dyCheck$'
      		</isNotNull>
           <isNotNull prepend="and" property="deCheck" >
           	v2.sc2='$deCheck$'
      		</isNotNull>
 order by END_ desc
