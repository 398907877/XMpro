<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd" >
<struts >
  <package name="deviceManagement" namespace="/deviceManagement" extends="default" >
    <action name="deviceManagementAction_*" class="deviceManagementAction" method="{1}" >
      <result name="deviceList" >
        /jsp/deviceManagement/deviceList.jsp
      </result>
      
      <result name="device" >
        /jsp/deviceManagement/device.jsp
      </result>
      
       <result name="downexl" >
        /jsp/deviceManagement/deviceList_downexl.jsp
      </result>
      
      <result name="downexl2" >
        /jsp/deviceManagement/detailList_downexl.jsp
      </result>
      
      <result name="detailList" >
        /jsp/deviceManagement/detailList.jsp
      </result>
    </action>
  </package>
</struts>