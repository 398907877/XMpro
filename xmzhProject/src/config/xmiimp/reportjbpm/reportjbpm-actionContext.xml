<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd" >
<beans >
  <bean name="postWorkloadStatisticsAction" id="postWorkloadStatisticsAction" class="com.gotop.reportjbpm.action.PostWorkloadStatisticsAction" singleton="false" >
    <property name="postWorkloadStatisticsService" ref="postWorkloadStatisticsService" />
  </bean>

  <bean name="errorStatistic" id="errorStatistic" class="com.gotop.reportjbpm.model.ErrorStatistic" singleton="false" ></bean>
  <bean name="errorStatisticAction" id="errorStatisticAction" class="com.gotop.reportjbpm.action.ErrorStatisticAction" singleton="false" >
    <property name="errorStatisticService" ref="errorStatisticService" />
    <property name="errorStatistic" ref="errorStatistic" />
  </bean>

  
  <bean name="reportRefusalrate" id="reportRefusalrate" class="com.gotop.reportjbpm.model.ReportRefusalrate" singleton="false" ></bean>
   <bean name="reportRefusalrateAction" id="reportRefusalrateAction" class="com.gotop.reportjbpm.action.ReportRefusalrateAction" singleton="false" >
    <property name="reportRefusalRateService" ref="reportRefusalRateService" />
    <property name="reportRefusalrate" ref="reportRefusalrate" />

  </bean>
  
  
  
  
</beans>